{
    "name": "catap97/nova-dependency-container",
    "description": "A Laravel Nova 4 form container for grouping fields that depend on other field values.",
    "keywords": [
        "laravel",
        "nova",
        "nova-4",
        "field"
    ],
    "authors": [
        {
            "name": "catap97",
            "email": "postacatalin3@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "CataP97\\DependencyContainer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CataP97\\DependencyContainer\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
